<Window x:Class="PL.privateMessagesWindow" Background="Ivory" ResizeMode="CanResize"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="privateMessagesWindow" Height="313.75" Width="300">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="180" Margin="10,66,0,0" VerticalAlignment="Top" Width="272"/>
        <Label x:Name="privateMsgLabel" FontFamily="cooper black" FontSize="30" Content="Private Messages" HorizontalAlignment="Left" Height="51" Margin="10,10,0,0" VerticalAlignment="Top" Width="272"/>
        <Button x:Name="backButton" FontFamily="cooper black" Content="back" HorizontalAlignment="Left" Height="18" Margin="43,251,0,0" VerticalAlignment="Top" Width="95" RenderTransformOrigin="0.905,-1.486" Click="backButton_Click"/>
        <Button x:Name="sengMessageButton" FontFamily="cooper black" Content="send message" HorizontalAlignment="Left" Height="18" Margin="154,251,0,0" VerticalAlignment="Top" Width="95" RenderTransformOrigin="0.905,-1.486" Click="sengMessageButton_Click"/>

    </Grid>
</Window>
